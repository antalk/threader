<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">
      
      <t:if test="isloggedin">
      	Edit account
     	<t:beaneditform t:id="accountedit" object="account" t:mixins="ic-t5/Bootstrap" exclude="id"/>
     	<p:else>
     		Create new account
     		<t:beaneditform t:id="accountcreate" object="newaccount" t:mixins="ic-t5/Bootstrap" exclude="id"/>
     	</p:else>
     </t:if>
</html>